{
  "plugins": [
    [
      "transform-react-remove-prop-types",
      {
        "mode": "unsafe-wrap",
        "ignoreFilenames": ["node_modules"]
      }
    ]
  ],
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false
      }
    ],
    "@babel/preset-react"
  ],
  "env": {
    "test": {
      "presets": ["@babel/preset-env", "@babel/preset-react"]
    }
  }
}
